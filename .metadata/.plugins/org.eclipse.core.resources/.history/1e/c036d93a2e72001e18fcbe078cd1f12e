package com.test.java;

import org.jsoup.nodes.Document;

public class Ex01 {

	public static void main(String[] args) {
	
		//Jsoup
		
		try {
			Document doc = Jsoup.connect("http://localhost:8090/memo/list.do");
			
			System.out.println(doc.html);
		} catch (Exception e) {
			e.printStackTrace();
		}
	}
	
}
